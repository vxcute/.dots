call plug#begin() 
    Plug 'morhetz/gruvbox'
    Plug 'vim-airline/vim-airline'
    Plug 'luisiacc/gruvbox-baby', {'branch': 'main'}
call plug#end()


set enc=utf-8
set fenc=utf-8
set termencoding=utf-8
set nocompatible
set autoindent
set smartindent
set tabstop=2       
set shiftwidth=2     
set expandtab        
syntax on
set number
set backspace=2
colorscheme gruvbox
set bg=dark
